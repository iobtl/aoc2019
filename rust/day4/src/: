fn main() {
    let x_min: u32 = 193651;
    let x_max: u32 = 649729;
    let x_curr: u32;
    let mut count: u32 = 0;

    for x_curr in x_min..x_max {
        let mut double: bool;
        let mut increasing_check: u32 = 0;
        let digits: Vec<u32> = x_curr.to_string()
            .chars()
            .map(|d| d.to_digit(10).unwrap())
            .collect();

        if double {
            println!("{} is suitable for a password", x_curr);
            count += 1;
        }
    }
}
